<a href="/createPost">글 작성하러 가기</a>

<div class="post">
    <ul>
            <li><b>제목</b></li>
        <% post.forEach( posts =>{ %>
            <li>
                <a href="/showOne/<%= posts._id %>" class="postTitle"><%= posts.title %></a>
                <a href="/updateOne/<%= posts._id %>">
                    <button>수정</button>
                </a>
                <form action="/deleteOne/<%= posts._id %>?_method=DELETE" method="POST">
                    <input type="submit" value="삭제">
                </form>
                
            </li>
        <%})%>
    </ul>
</div>